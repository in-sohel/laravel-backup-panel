{
    "name": "pavel-mironchik/laravel-backup-panel",
    "description": "An interface for Spatie Laravel Backup package",
    "homepage": "https://github.com/YOUR_GITHUB_USERNAME/laravel-backup-panel",
    "keywords": [
        "backup",
        "database",
        "laravel-backup",
        "spatie"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Mironchik",
            "email": "mironchikpavel@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "livewire/livewire": "^3.0",
        "spatie/laravel-backup": "^8.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PavelMironchik\\LaravelBackupPanel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PavelMironchik\\LaravelBackupPanel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PavelMironchik\\LaravelBackupPanel\\LaravelBackupPanelServiceProvider"
            ]
        }
    }
}
